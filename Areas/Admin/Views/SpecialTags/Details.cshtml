@model B2BApp.Models.SpecialTags
@{
    ViewData["Title"] = "Details";
}
<br/>

<h2 class="text-info">Detalii Producator</h2>
<br/>
<form asp-action="Details" method="post">
    <div class="p-4 rounded border">
        <div asp-validation-sumarry="ModelOnly" class="text-danger">

        </div>
        <div class="form-group row">
            <div class="col-md-2">
                <label asp-for="SpecialTag">Producator</label>
            </div>
            <div class="col-md-5">
                <input asp-for="SpecialTag" readonly="readonly" class="form-control"/>
            </div>
            <span asp-validation-for="SpecialTag" class="text-danger"></span>
        </div>
        <div class="form-group">
            
            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editeaza</a>
            <a asp-action="Index" class="btn btn-success">Inapoi la lista</a>
            @*<input type="submit" class="btn  btn-success" value="Back to List"/>*@
        </div>
    </div>
</form>

@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
